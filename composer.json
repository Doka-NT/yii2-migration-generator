    "description": "Generates migrations from Annotations",
    "license": "MIT",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "authors": [
        {
            "name": "Soshnikov Artem",
            "email": "213036@skobka.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "yiisoft/yii2": "~2.0",
        "doctrine/annotations": "v1.2.7",
        "crazycodr/standard-exceptions": "^2.1"
    },

    "autoload": {
        "psr-4": {
            "skobka\\yii2\\migrationGenerator\\": "src"
        }
